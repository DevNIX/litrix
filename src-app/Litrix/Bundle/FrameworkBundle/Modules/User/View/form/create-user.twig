 <div class="col-lg-12">
    <div class="panel panel-default">
        <div class="panel-heading ">
            <h3>Nuevo Cliente</h3>
        </div>
        <div class="panel-body">
            <form class="form-inline" role="form" action="{{ app.request.basepath }}/admin/usercreate" name="create_user_form" id="create_user_form" method="post" {{ form_enctype(form_create_user) }} >
                <div class="form-group col-lg-12" style = "margin-bottom: 30px; margin-top: 30px;">

                    {{ form_widget(form_create_user._token) }}
                    {{ form_label(form_create_user.nombre,"Nombre",{'label_attr':{'class':'control-label'}}) }}
                    {{ form_errors(form_create_user.nombre) }}
                    {{ form_widget(form_create_user.nombre) }}

                    {{ form_label(form_create_user.apellidos,"Apellidos",{'label_attr':{'class':'control-label'}})  }}
                    {{ form_errors(form_create_user.apellidos) }}
                    {{ form_widget(form_create_user.apellidos) }}

                    {{ form_label(form_create_user.mail,"Email",{'label_attr':{'class':'control-label'}})  }}
                    {{ form_errors(form_create_user.mail) }}
                    {{ form_widget(form_create_user.mail) }}
                </div>
                <div class="form-group col-lg-12" style = "margin-bottom: 30px;">
                    {{ form_label(form_create_user.usuario,"Usuario",{'label_attr':{'class':'control-label'}})   }}
                    {{ form_errors(form_create_user.usuario) }}
                    {{ form_widget(form_create_user.usuario) }}

                    {{ form_label(form_create_user.pass,"Contrase√±a",{'label_attr':{'class':'control-label'}})  }}
                    {{ form_errors(form_create_user.pass) }}
                    {{ form_widget(form_create_user.pass) }}

                </div>
                <div class="form-group col-lg-12" style = "margin-bottom: 30px;">
                    <button id="create_user_form_submit"  class="btn btn-primary">Crear Usuario</button>
                    <a class="btn btn-warning" href="{{ path('user_admin') }}">Cancelar</a>
                </div>
            </form>
        </div>
    </div>
 </div>